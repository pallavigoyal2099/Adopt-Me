{
    "extends":["eslint:recommended", 
        "plugin:import/errors",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "prettier",
        "prettier/react"
    ],
    "plugins":["react","import","jsx-a11y","react-hooks"],
    "parser": "babel-eslint",

    

    "parserOptions":{
        "ecmaVersion":2020,
        "sourceType": "module",
        "ecmaFeatures":{
            "jsx":true
        }

    },
    "rules": {
        "react/prop-types":0,
        "no-console":1

        ,"react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps":1
    },
    "env":{
        "es6":true,
        "browser":true,
        "node":true
    },
    "settings": {
        "react":"detect"
    }
    
}